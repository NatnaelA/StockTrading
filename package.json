{
  "name": "stocktrading",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "mobile",
    "web",
    "functions",
    "shared"
  ],
  "scripts": {
    "mobile": "cd mobile && npm start",
    "web": "cd web && npm run dev",
    "functions": "cd functions && npm run serve",
    "test": "npm run test:mobile && npm run test:web",
    "test:mobile": "cd mobile && npm test",
    "test:web": "cd web && npm test",
    "build": "npm run build:web && npm run build:mobile",
    "build:web": "cd web && npm run build",
    "build:mobile": "cd mobile && npm run build",
    "lint": "npm run lint:web && npm run lint:mobile",
    "lint:web": "cd web && npm run lint",
    "lint:mobile": "cd mobile && npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Stock Trading Platform with React Native mobile app and Next.js web dashboard",
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "firebase": "^11.2.0",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0"
  },
  "devDependencies": {
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@types/testing-library__user-event": "^4.1.1"
  }
}
